package abstruct;

import javax.swing.DefaultBoundedRangeModel;

abstract class P2 
{
	abstract void show();
	 void x()
	{
		System.out.println("Method Called...");
	}
}

class P1 extends P2
{
	void show() 
	{
		System.out.println("Method Overriding...");
	}
	
	public static void main(String[] args) {
		
		P1 p = new P1();
		p.show();
		p.x();
	}
}




