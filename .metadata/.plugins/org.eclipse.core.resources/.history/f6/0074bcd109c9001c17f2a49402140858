package methodoverriding;

class Xyz
{
	void A()
	{
		System.out.println("Parent Method Called...");
		return 0;
	}
}

public class Test extends Xyz
{
	void A()
	{
		System.out.println("Child Method Called...");
		return 0;
	}
	public static void main(String[] args) {
		
		Xyz x = new Xyz();
		x.A();
		
		Test t = new Test();
		t.A();
	}
}
