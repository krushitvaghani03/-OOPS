package Super;

class P
{
	public P(int i) 
	{
		System.out.println("Parent Constructor" + i);
	}
}

public class Constructor extends P
{
	public Constructor() 
	{
		super(10);
		System.out.println("Child Constructor");
	}
	
	public static void main(String[] args) {
		
		Constructor c = new Constructor();
		
	}
}
